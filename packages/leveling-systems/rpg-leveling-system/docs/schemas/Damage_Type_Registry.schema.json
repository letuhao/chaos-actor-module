{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Damage Type Registry",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "types": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "key"
        ],
        "properties": {
          "key": {
            "type": "string"
          },
          "inherits": {
            "type": "string"
          }
        }
      }
    }
  }
}